{
  "manifest_version": 2,

  "name": "Slithered Bot",
  "description": "A sample Slither Bot for slither.io",
  "version": "1.0",
  "background":{
    "scripts" : ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Slithered Bot"
    // "default_popup": "popup.html"
  },
  "permissions": [
    "background",
    "activeTab",
    "http://*/*",
    "https://*/*",
    "https://ajax.googleapis.com/"
  ]
}